// Notifications and Notification Groups 
// 
// Keys are the key of the notification plus an appended _name or _desc
// prefix indicating if this is the name or description part. 
// 


// Navigational group used for requests that change between desktops and to 
// show navigational helpers like the pager
VTGroupNavigation_name					= "Navigation"; 

// Desktop manipulation group containing operations on desktops 
VTGroupDesktops_name					= "Manipulation des bureaux";
	
// Inspectors... 
VTGroupInspection_name					= "Inspecteurs";


// Request to change the desktop to a named desktop. Name and Description take
// the desktop name as the only format string parameter. 
VTRequestChangeDesktop_name				= "Aller au bureau '%@'"; 
VTRequestChangeDesktop_desc				= "Aller au bureau nomé '%@'";
	
// Request to change the desktop to the previous desktop. 
VTRequestChangeDesktopToLast_name		= "Aller au précedent"; 
VTRequestChangeDesktopToLast_desc		= "Aller au bureau précédement utilisé";

// Request to change the desktop to the desktop marked as the Snapback desktop
VTRequestChangeDesktopToSnapback_name	= "Aller au bureau Snapback"; 
VTRequestChangeDesktopToSnapback_desc	= "Aller au bureau qui a été marqué comme étant le bureau Snapback"; 
	
// Request to set the currently visible desktop as the Snapback desktop 
VTRequestSetSnapbackDesktop_name		= "Définir le Snapback"; 
VTRequestSetSnapbackDesktop_desc		= "Définis le bureau courant comme étant le bureau Snapback"; 
	
// Request to change to the desktop to the east (the right)
VTRequestChangeDesktopToEast_name		= "Aller à l'est";
VTRequestChangeDesktopToEast_desc		= "Aller au bureau à l'est (droite) du bureau actuel"; 
	
// Request to change to the desktop to the west (the left) 
VTRequestChangeDesktopToWest_name		= "Aller à l'ouest";
VTRequestChangeDesktopToWest_desc		= "Aller au bureau à l'ouest (gauche) du bureau actuel"; 
	
// Request to change to the desktop to the north (above) 
VTRequestChangeDesktopToNorth_name		= "Aller au nord";
VTRequestChangeDesktopToNorth_desc		= "Aller au bureau au nord (au dessus) du bureau actuel";
	
// Request to change to the desktop to the south (below)
VTRequestChangeDesktopToSouth_name		= "Aller au sud";
VTRequestChangeDesktopToSouth_desc		= "Aller au bureau au sud (en dessous) du bureau actuel";
	
// Request to show the overlay pager in Normal-Mode
VTRequestShowPager_name					= "Afficher le navigateur"; 
VTRequestShowPager_desc					= "Afficher le navigateur qui permet de visualiser et de se déplacer dans les buraux disponibles";
	
// Request to show the overlay pager in Sticky-Mode 
VTRequestShowPagerAndStick_name			= "Afficher le navigateur en mode collant"; 
VTRequestShowPagerAndStick_desc			= "Afficher le navigateur en mode collant. Le mode collant permet d'utiliser le navigateur sans maintenir une touche enfoncée."; 
	
// Request to inspect Desktops using the Desktops Inspector (Deprecated) 
VTRequestInspectDesktops_name			= "Inspecter les bureaux.";
VTRequestInspectDesktops_desc			= "Affiche les bureaux disponibles. Cela permet de renommer, ajouter ou supprimer des bureaux.";
	
// Request to inspect the visible desktop using the Desktop Inspector 
VTRequestInspectDesktop_name			= "Inspecter les Bureaux"; 
VTRequestInspectDesktop_desc			= "Affiche les informations spécifiques à un bureau. Ces informations sont, par exemple, la liste des applications ouvertes sur ce bureau."; 
	
// Request to inspect Applications using the Application Inspector 
VTRequestInspectApplications_name		= "Inspecter les Applications"; 
VTrequestInspectApplications_desc		= "Affiche les préférences liées à une Application particulière."; 
	
// Request to inspect preferences 
VTRequestInspectPreferences_name		= "Inspecter les Préférences"; 
VTRequestInspectPreferences_desc		= "Affiche les préférences de Virtue."; 
	
// Request to toggle visiblity of the statusbar menu 
VTRequestToggleStatusbarMenu_name		= "Alterner la visibilité dans la barre des menus";
VTRequestToggleStatusbarMenu_desc		= "Permet d'afficher ou de masquer l'état de Virtue dans la barre des menus."; 
	
// Request to delete the visible desktop 
VTRequestDeleteDesktop_name				= "Détruire le bureau courant";
VTRequestDeleteDesktop_desc				= "Détruit le bureau courant et passe au bureau précédent. Il est possible de choisir de bouger les fenêtres de ce bureau vers le bureau precedent.";
	
// Request to show the overlay window of the window under the mouse pointer 
VTRequestDisplayOverlay_name			= "Afficher le navigateur sous le curseur"; 
VTRequestDisplayOverlay_desc			= "Affiche le navigateur sous le curseur. Cela permet de bouger les fenêtres des applications entre les bureaux ou encore de naviger entre les bureaux."; 